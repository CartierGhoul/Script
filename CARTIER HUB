local MainTab = Window:CreateTab("Amore", 4483362458) -- Title, Image
local MainSection = MainTab:CreateSection("Main")
Rayfield:Notify({
	Title = "Executed KNB hub by campy",
	Content = "Hog Ridaaa",
	Duration = 6.5,
	Image = 4483362458,
	Actions = { -- Notification Buttons
