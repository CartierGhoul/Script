						Highlight.Enabled = false
					end
				elseif not footing and CoreGui:FindFirstChild("Highlight") then
					CoreGui:FindFirstChild("Highlight").Enabled = false
				end
			elseif not hasBall and CoreGui:FindFirstChild("Highlight") then
				CoreGui:FindFirstChild("Highlight").Enabled = false
			end
		end

		function getGoal()
			local dist, goal = 9e9
			for i,v in pairs(workspace:GetDescendants()) do
				if v.Name == "Goal" or v.Name == "SHEESH" or v.Name == "Rim" then
					local mag = (plr.Character.HumanoidRootPart.Position - v.Position).Magnitude
					if dist > mag then
						dist = mag; goal = v;
					end
				end
			end
			return goal
		end

		function getNewVector(goal)
			local dist = (plr.Character.Torso.Position - goal.Position).Magnitude
			return goal.Position + Vector3.new(0, 45 - (dist / 10), 0) - plr.Character.Humanoid.MoveDirection
		end

		function shootBall(vector)
			local event = plr.Character.Basketball.shoot_event

			local unit = (vector - plr.Character.Head.Position).Unit
			local spawnPos = plr.Character.PrimaryPart.Position + unit * 3.8

			if spawnPos.Y - plr.Character.PrimaryPart.Position.Y < 4 then
				spawnPos = plr.Character.PrimaryPart.Position + unit * 4
			end

			event:FireServer(
				vector,
				spawnPos,
				"\240\159\148\165\240\159\148\165"
			)
		end

		function handleJumped()
			if hasBall and footing or almostFooting and _G.Enabled then
				task.wait(0.3)
				shootBall(getNewVector(getGoal()))
			end
		end

		function handleRender()
			if not _G.Enabled or _G.torso == nil then
				return
			end

			local goal = getGoal()
			local dist = (plr.Character.Torso.Position - goal.Position).Magnitude

			if math.floor(dist) == 59 or math.floor(dist) == 64 then
				footing = true
				almostFooting = false
			elseif math.floor(dist) == 58 or math.floor(dist) == 60 then
				almostFooting = true
				footing = false
			else
				footing = false
				almostFooting = false
			end

			if plr.Character and plr.Character:FindFirstChild("Basketball") then
				task.wait(0.5)
				hasBall = true
			else
				hasBall = false
			end

			checkFooting()
			_G.torso.Position = plr.Character:WaitForChild("Torso").Position
			_G.torso.Rotation = plr.Character:WaitForChild("Torso").Rotation
		end

		-- Connections

		_G.hC = plr.Character.Humanoid.Jumping:Connect(handleJumped)
		_G.rsC = rs.Stepped:Connect(handleRender)

		plr.CharacterAdded:Connect(function()
			if not _G.Enabled then
				return
			end

			_G.hC:Disconnect()
			_G.hC = nil
			_G.rsC:Disconnect()
			_G.rsC = nil

			_G.hC = plr.Character:WaitForChild("Humanoid").Jumping:Connect(handleJumped)
			_G.rsC = rs.Stepped:Connect(handleRender)
		end)

		plr.PlayerGui.DescendantAdded:Connect(function(v)
			if not _G.Enabled then
				return
			end

			if plr.PlayerGui:FindFirstChild("Tracker") and plr.PlayerGui:FindFirstChild("Loop") then
				for i,v in pairs(getconnections(plr.PlayerGui.Tracker.Changed)) do
					v:Disable();
				end
				for i,v in pairs(getconnections(plr.PlayerGui.Loop.Changed)) do
					v:Disable();
				end
				for i,v in pairs(getconnections(plr.PlayerGui.Loop.AncestryChanged)) do
					v:Disable();
				end
				for i,v in pairs(getconnections(plr.PlayerGui.Tracker.AncestryChanged)) do
					v:Disable();
				end

				plr.PlayerGui.Tracker.Disabled = true
				plr.PlayerGui.Loop.Disabled = true
				plr.PlayerGui.Tracker:Destroy()
				plr.PlayerGui.Loop:Destroy()
			end
		end)

		-- PVEye Disabler

		pcall(function()
			if plr.PlayerGui:FindFirstChild("Tracker") and plr.PlayerGui:FindFirstChild("Loop") and _G.Enabled then
				for i,v in pairs(getconnections(plr.PlayerGui.Tracker.Changed)) do
					v:Disable();
				end
				for i,v in pairs(getconnections(plr.PlayerGui.Loop.Changed)) do
					v:Disable();
				end
				for i,v in pairs(getconnections(plr.PlayerGui.Loop.AncestryChanged)) do
					v:Disable();
				end
				for i,v in pairs(getconnections(plr.PlayerGui.Tracker.AncestryChanged)) do
					v:Disable();
				end

				plr.PlayerGui.Tracker.Disabled = true
				plr.PlayerGui.Loop.Disabled = true
				plr.PlayerGui.Tracker:Destroy()
				plr.PlayerGui.Loop:Destroy()
			end
		end
		)
	end,
})

local Button = SixthTab:CreateButton({
	Name = "BAU CAMLOCK",
	Callback = function()
		local tabDown = false
		local input = game:GetService('UserInputService')
		local player = game.Players.LocalPlayer
		local camera = game.Workspace.CurrentCamera
		local character = player.Character 
		local head = character.Head
		local torso = character.Torso
		pos = Vector3.new(0,0,0)
		local rs = game:GetService('RunService')
		isdoing = false

		cor = coroutine.create(function()
			while wait() do
				if tabDown and I == 0 then
					local pos = head.Position
					local partyGoal = findGoal(head.Position)
					local lookAt = partyGoal.Position + Vector3.new(0,37,0)
					local mag = (character.PrimaryPart.Position - partyGoal.Position).magnitude
					local cameraCFrame = CFrame.new(pos, lookAt)
					camera.CFrame = cameraCFrame
					I = I + 1
					rs.RenderStepped:Wait()
					I = I - 1
					if isdoing == false then
						coroutine.yield()
					end
				end
			end



		end)

		function findGoal(position)
			local nearestGoal = nil
			local lowest = math.huge
			for i,v in pairs(game.Workspace:GetDescendants()) do
				if v.Name == 'Goal' and v:IsA('Part') then
					local distance = (torso.Position - v.Position).magnitude
					if distance < lowest then
						lowest = distance
						Goal = v
					end
				end
			end
			return Goal
		end

		input.InputBegan:Connect(function(key, gpe)
			local xkey = key.KeyCode
			if xkey == Enum.KeyCode.X then
				tabDown = true
				if isdoing == false then
					wait()
					isdoing = true 
					coroutine.resume(cor)
				end
			end
		end)

		input.InputEnded:Connect(function(key, gpe)
			local xkey = key.KeyCode
			if xkey == Enum.KeyCode.X then
				tabDown = false
				if isdoing then
					isdoing = false
				end 
			end 
		end)
		I = 0
	end
})
