local Button = FourthTab:CreateButton({
	Name = "Vedden Aimbot",
	Callback = function()
		local SeventyFive = {
			Distance = 65,
			Power = 75,	
		}

		local Self = game.Players.LocalPlayer.Character.HumanoidRootPart
		local Player = game.Players.LocalPlayer
		local Humanoid = Player.Character.Humanoid
		local char = Player.Character
		local Camera = workspace.CurrentCamera

		isdoing = false



		game:GetService("UserInputService").InputBegan:connect(function(key, Processed)
			if Processed then return end


			if key.KeyCode == Enum.KeyCode.N then

				local Tool = game.Players.LocalPlayer.Character:FindFirstChild("Ball")
				if not Tool then return end

				local events = game.ReplicatedStorage.Animations.withball
				local ShootingFormRelease = Humanoid:LoadAnimation(events.ShootingFormRelease)
				local ShootingFormTrack = Humanoid:LoadAnimation(events.ShootingForm)


				for _,v in pairs(workspace.Hoops:GetDescendants()) do

					local function cam(amount)
						while wait() do
							if isdoing == true then
								local pos = char.Head.Position
								local partyGoal = v
								local lookAt = v.Position + Vector3.new(0,amount,0)
								local cameraCFrame = CFrame.new(pos, lookAt)
								Camera.CFrame = cameraCFrame
							else
								return nil
							end
						end
					end


					if v.Name == "Hax" then
						if (v.Position - Self.Position).Magnitude <= 63 and (v.Position - Self.Position).Magnitude  >= 60 then -- 3P

							game.ReplicatedStorage.adjustpower:FireServer(Player.Name, 75)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,200,0)
							isdoing = true
							cam(55)

						elseif (v.Position - Self.Position).Magnitude <= 53 and (v.Position - Self.Position).Magnitude  >= 51 then -- Deep Midrange

							game.ReplicatedStorage.adjustpower:FireServer(Player.Name, 65)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,50,0)
							isdoing = true
							cam(40)

						elseif (v.Position - Self.Position).Magnitude <= 55 and (v.Position - Self.Position).Magnitude  >= 53 then -- Deep Midrange

							game.ReplicatedStorage.adjustpower:FireServer(Player.Name, 65)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,50,0)
							isdoing = true
							cam(32)

						elseif (v.Position - Self.Position).Magnitude <= 51 and (v.Position - Self.Position).Magnitude  >= 49 then -- Fade

							game.ReplicatedStorage.adjustpower:FireServer(Player.Name, 65)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,50,0)
							isdoing = true
							cam(30)

						elseif (v.Position - Self.Position).Magnitude <= 49 and (v.Position - Self.Position).Magnitude  >= 47 then -- Fade

							game.ReplicatedStorage.adjustpower:FireServer(Player.Name, 65)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,50,0)
							isdoing = true
							cam(50)

						elseif (v.Position - Self.Position).Magnitude <= 47 and (v.Position - Self.Position).Magnitude  >= 45 then -- Fade

							game.ReplicatedStorage.adjustpower:FireServer(Player.Name, 60)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,50,0)
							isdoing = true
							cam(40)

						elseif (v.Position - Self.Position).Magnitude <= 41 and (v.Position - Self.Position).Magnitude  >= 38 then -- Fade

							game.ReplicatedStorage.adjustpower:FireServer(Player.Name, 60)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,50,0)
							isdoing = true
							cam(50)

						elseif (v.Position - Self.Position).Magnitude <= 32 and (v.Position - Self.Position).Magnitude  >= 29 then -- High Layup

							game.ReplicatedStorage.adjustpower:FireServer(Player.Name, 35)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,50,0)
							isdoing = true
							cam(5)

