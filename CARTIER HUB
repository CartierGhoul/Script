local Button = MainTab:CreateButton({
	Name = "Amore Aimbot",
	Callback = function()
		local SeventyFive = {
			Distance = 65,
			Power = 75,	
		}

		local Self = game.Players.LocalPlayer.Character.HumanoidRootPart
		local Player = game.Players.LocalPlayer
		local Humanoid = Player.Character.Humanoid
		local char = Player.Character
		local Camera = workspace.CurrentCamera

		isdoing = false



		game:GetService("UserInputService").InputBegan:connect(function(key, Processed)
			if Processed then return end


			if key.KeyCode == Enum.KeyCode.N

			then local  Tool = game.Players.LocalPlayer.Character:FindFirstChild("Basketball")
				if not Tool then return end

				for _,v in pairs(workspace:GetDescendants()) do

					local function cam(amount)
						while wait() do
							if isdoing == true then
								local pos = char.Head.Position
								local partyGoal = v
								local lookAt = v.Position + Vector3.new(0,amount,0)
								local cameraCFrame = CFrame.new(pos, lookAt)
								Camera.CFrame = cameraCFrame
							else
								return nil
							end
						end
					end


					if v.Name == "Dunk" then
						if (v.Position - Self.Position).Magnitude <= 63.5 and (v.Position - Self.Position).Magnitude  >= 61 then -- 3P

							game.ReplicatedStorage.Events.Core["Adjust Power"]:FireServer(85)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,200,0)
							isdoing = true
							cam(80)
						elseif (v.Position - Self.Position).Magnitude <= 64 and (v.Position - Self.Position).Magnitude  >= 63.5 then -- 3P

							game.ReplicatedStorage.Events.Core["Adjust Power"]:FireServer(85)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,200,0)
							isdoing = true
							cam(80)

						elseif (v.Position - Self.Position).Magnitude <= 65 and (v.Position - Self.Position).Magnitude  >= 64 then -- 3P

							game.ReplicatedStorage.Events.Core["Adjust Power"]:FireServer(85)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,200,0)
							isdoing = true
							cam(80)

						elseif (v.Position - Self.Position).Magnitude <= 66 and (v.Position - Self.Position).Magnitude  >= 65 then -- 3P

							game.ReplicatedStorage.Events.Core["Adjust Power"]:FireServer(85)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,200,0)
							isdoing = true
							cam(75)

						elseif (v.Position - Self.Position).Magnitude <= 67 and (v.Position - Self.Position).Magnitude  >= 66 then -- 3P

							game.ReplicatedStorage.Events.Core["Adjust Power"]:FireServer(85)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,200,0)
							isdoing = true
							cam(75)

						elseif (v.Position - Self.Position).Magnitude <= 68 and (v.Position - Self.Position).Magnitude  >= 67 then -- 3P

							game.ReplicatedStorage.Events.Core["Adjust Power"]:FireServer(85)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,200,0)
							isdoing = true
							cam(75)

						elseif (v.Position - Self.Position).Magnitude <= 69 and (v.Position - Self.Position).Magnitude  >= 68 then -- 3P

							game.ReplicatedStorage.Events.Core["Adjust Power"]:FireServer(85)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,200,0)
							isdoing = true
							cam(65)

						elseif (v.Position - Self.Position).Magnitude <= 70 and (v.Position - Self.Position).Magnitude  >= 69 then -- 3P

							game.ReplicatedStorage.Events.Core["Adjust Power"]:FireServer(85)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,200,0)
							isdoing = true
							cam(65)

						elseif (v.Position - Self.Position).Magnitude <= 71 and (v.Position - Self.Position).Magnitude  >= 70 then -- 3P

							game.ReplicatedStorage.Events.Core["Adjust Power"]:FireServer(85)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,200,0)
							isdoing = true
							cam(65)

						elseif (v.Position - Self.Position).Magnitude <= 72 and (v.Position - Self.Position).Magnitude  >= 71 then -- 3P

							game.ReplicatedStorage.Events.Core["Adjust Power"]:FireServer(85)
							local A1 = v.Position - Tool.Basketball.Position *(100/200) + Vector3.new(0,200,0)
							isdoing = true
							cam(60)










						end	
					end
				end	
			end
		end)

		local UIS = game:service('UserInputService')
		UIS.InputBegan:connect(function(l)
			if l.UserInputType == Enum.UserInputType.MouseButton1 then
				isdoing = false
			end
		end)

		local tr = false
	end,
})

local Slider = MainTab:CreateSlider({
	Name = "AmoreSpeed",
	Range = {0, 17},
	Increment = 1,
	Suffix = "Speed",
	CurrentValue = 16,
	Flag = "Slider1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (Value)
	end,
})

local Button = MainTab:CreateButton({
	Name = "Lag switch (X TO USE)",
	Callback = function()
		loadstring(game:HttpGet("https://[Log in to view URL]", true))()
	end,
})

local Button = MainTab:CreateButton({
	Name = "Auto Guard",
	Callback = function()
		local tr = false

		game:GetService("UserInputService").InputBegan:Connect(function(key, gpe)
			if key.KeyCode == Enum.KeyCode.X and not gpe and tr == false then
				tr = true
				for i, v in pairs(game.Players:GetPlayers()) do
					local char = game.Players.LocalPlayer.Character
					if v.Name ~= char.Name and (v.Character.Torso.CFrame.p - char.Torso.CFrame.p).Magnitude < 25 then
						while tr == true do
							if v.Character.Torso.Velocity.magnitude > 0.5 and v.Character:FindFirstChild("Basketball") then
								char.Humanoid:MoveTo(v.Character.Torso.CFrame.p + v.Character.Torso.Velocity.unit * 7)
							elseif v.Character.Torso.Velocity.magnitude < 0.5 and v.Character:FindFirstChild("Basketball") then
								char.Humanoid:MoveTo(v.Character.Torso.CFrame.p)
							elseif not v.Character:FindFirstChild("Basketball") then
								tr = false
							end
							task.wait()
						end
					end
				end
			elseif key.KeyCode == Enum.KeyCode.X and not gpe and tr == true then
				tr = false
			end
		end
		)
	end,
})

